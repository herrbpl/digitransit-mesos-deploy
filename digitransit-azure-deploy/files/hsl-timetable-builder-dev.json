{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "hsldevcom/hsl-timetable-builder",
      "network": "HOST",
      "parameters": [],
      "portMappings": null,
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/run/docker.sock",
        "hostPath": "/var/run/docker.sock",
        "mode": "RO"
      }
    ]
  },
  "cpus": 1,
  "dependencies": [],
  "disk": 0,
  "env": {
    "DOCKER_AUTH": "dockerauth",
    "SLACK_WEBHOOK_URL": "slackwebhook",
    "BUILDER_TYPE": "dev",
    "DOCKER_USER": "dockeruser",
    "DOCKER_TAG": "latest",
    "TIMETABLE_DAYS_ADVANCE": "2",
    "FONTSTACK_PASSWORD": "fontstacksecret"
  },
  "gpus": 0,
  "healthChecks": [
    {
      "command": {
        "value": "true"
      },
      "gracePeriodSeconds": 30,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 2,
      "protocol": "COMMAND",
      "timeoutSeconds": 20
    }
  ],
  "id": "/hsl-timetable-builder",
  "instances": 1,
  "labels": {
    "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 1024,
  "portDefinitions": [
    {
      "labels": {},
      "port": 10030,
      "protocol": "tcp"
    }
  ],
  "ports": [
    10030
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0.5
  }
}
